<%= render :partial => 'shared/interior_hero' %>
<div id="interior_wrapper">
    <div id="interior_header_absolute">
        <h6 id="int_header">New Staff</h6>
    </div>
    
    <div id="interior_sub_head"><h5 id="sub_green_head"></h5></div>

    
    <div id="interior_pad">

<h1>New staff</h1>

<%= form_for @staff, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :bio %><br />
    <%= f.text_area :bio %>
  </p>
  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link %>
  </p>
  <p><%= f.label :photo %><br>
      <%= f.file_field :photo %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', staffs_path %>
    </div>

</div>

<%= render :partial => 'shared/interior_footer' %>
